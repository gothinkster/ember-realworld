{
  "name": "ember-realworld",
  "version": "0.1.0",
  "private": true,
  "description": "This codebase was created to demonstrate a fully fledged fullstack application built with **Ember** including CRUD operations, authentication, routing, pagination, and more.",
  "repository": "https://github.com/gothinkster/ember-realworld",
  "license": "MIT",
  "author": "Alon Bukai",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [

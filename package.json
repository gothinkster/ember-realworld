    "loader.js": "^4.7.0",
    "marked": "^4.0.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^1.10.0",
    "qunit": "^2.13.0",
    "qunit-dom": "^1.6.0",
    "sinon": "^8.1.1"
